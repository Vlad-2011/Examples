#include <stdio.h>

void InsertionSort(int arr[], int n) {
	int temp, j;
    
    for (int i = 1; i < n; i++) {
    	temp = arr[i];
        j = i - 1;
        while (j >= 0 && arr[j] > temp) {
        	arr[j + 1] = arr[j];
            j--;
        }
        arr[j + 1] = temp;
    }
}

int main() {
    int a[100];
    for (int i = 0; i < 100; i++) {
    	a[i] = 100 - i;
    }
    InsertionSort(a, 100);
    for (int i = 0; i < 100; i++) {
    	printf("%i ", a[i]);
    }
    printf("\n");
    
    return 0;
}
