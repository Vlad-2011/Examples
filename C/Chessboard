#include <stdio.h>
#include <stdlib.h>

int main()
{
    int x1;
    int y1;
    int x2;
    int y2;
    
    printf("X первой клетки: ");
    scanf("%i", &x1);
    printf("Y первой клетки: ");
    scanf("%i", &y1);
    printf("X второй клетки: ");
    scanf("%i", &x2);
    printf("Y второй клетки: ");
    scanf("%i", &y2);
    
    if (abs(x1 - x2) <= 1 && abs(y1 - y2) <= 1)
    {
        printf("YES");
    }
    else
    {
        printf("NO");
    }
}
