def max(a, b):
    if a > b:
        return a
    elif a < b:
        return b
    else:
        return 0

def max3(a, b, c):
    return max(max(a, b), c)

print(max3(3, 5, 4))

def f():
    global a
    a = 1
    print(a)
a = 0
f()
print(a)

def factorial(n):
    res = 1
    for i in range(2, n+1):
        res *= i
    return res

n = int(input())
f = factorial(n)
print(f)
